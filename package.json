{
  "name": "shopping_list_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend": ". ~/.nvm/nvm.sh && nvm use 18 && node server.js",
    "dev:all": "bash -c 'unset npm_config_prefix && . ~/.nvm/nvm.sh && nvm use 18 && concurrently \"npm run dev\" \"node server.js\"'",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": ". ~/.nvm/nvm.sh && nvm use 18 && node server.js",
    "install-browsers": "playwright install chromium"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.17.1",
    "@react-google-maps/api": "^2.19.3",
    "@types/leaflet": "^1.9.17",
    "@types/node": "^20.11.19",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "geolocation-utils": "^1.2.6",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^2.15.0",
    "openai": "^3.2.1",
    "playwright": "^1.51.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@types/google.maps": "^3.58.1",
    "@types/mapbox-gl": "^3.4.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^13.24.0",
    "typescript": "~5.3.3",
    "typescript-eslint": "^7.0.1",
    "vite": "^5.1.3"
  }
}
